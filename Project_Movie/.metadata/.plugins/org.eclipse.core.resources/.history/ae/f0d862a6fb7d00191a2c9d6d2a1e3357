package MultiWerverClient;

import java.net.InetSocketAddress;
import java.net.ServerSocket;
import java.net.Socket;
import java.util.HashMap;

class Info{
	
	
	
	
	
}

public class MultiServer {
	
	public static void main(String[] args) {
		    HashMap<String,String> db = new HashMap<String, String>();
		
		
		try {
			HashMap<String, > hm = new HashMap();
			ServerSocket server = new ServerSocket();
			InetSocketAddress ipep = new InetSocketAddress(10001);
			server.bind(ipep);
			
			while(true) {
				System.out.println("Movie 서버를 시작합니다. User의 접속을 기다립니다.");
				Socket sock = server.accept();
				System.out.println(sock.getInetAddress() + "님이 접속하셨습니다.");
			}
			
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
